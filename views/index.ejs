    <link href="https://fonts.googleapis.com/css?family=Montserrat:400,700,200" rel="stylesheet" />
    <link href="https://maxcdn.bootstrapcdn.com/font-awesome/latest/css/font-awesome.min.css" rel="stylesheet">

</head>

<body onresize="configview();">

    <nav class="navbar navbar-default" role="navigation-demo" id="demo-navbar">
        <div class="container">
            <!-- Brand and toggle get grouped for better mobile display -->
            <div class="navbar-header">
                <button type="button" class="navbar-toggle" data-toggle="collapse" data-target="#navigation-example-2">
                    <span class="sr-only">Toggle navigation</span>
                    <span class="icon-bar"></span>
                    <span class="icon-bar"></span>
                    <span class="icon-bar"></span>
                </button>
                <a class="navbar-brand" href="#">Create in 30 minutes</a>
            </div>

            <!-- Collect the nav links, forms, and other content for toggling -->
            <div class="collapse navbar-collapse" id="navigation-example-2">
                <ul class="nav navbar-nav navbar-right">
                    <li>
                        <a href="/login" class="btn btn-simple">Sign in</a>
                    </li>
                    <li>
                        <a href="/register" class="btn btn-simple">Sign up</a>
                    </li>
                </ul>

            </div><!-- /.navbar-collapse -->
        </div><!-- /.container-->
    </nav>
    <div id="container">
        <div class="wrapper">
            <div class="landing-header"
                style="background-image: url('/img/index.webp');">
                <div class="container">
                    <div class="motto">
                        <h1 class="title-uppercase">Create in 30 minutes</h1>
                        <h3>Em nghi ra mot so cai core value nao do de viet o day</h3>
                    </div>
                </div>
            </div>
            <div class="main">
                <div class="section text-center landing-section">
                    <div class="container">
                        <!-- diem den hang dau -->
                        <div class="row">
                            <div class="col-md-8 col-md-offset-2">
                                <h2>Top destinations</h2>
                            </div>
                        </div>
                        <div class="row example-pages">
                            <div class="col-md-4 one-location">
                                <a href="#hanoi" target="_blank">
                                    <img src="/img/hoguom.jpg" alt="Rounded Image" class="img-rounded img-responsive">
                                    <h5>Hà Nội</h5>
                                </a>
                            </div>
                            <div class="col-md-4 one-location">
                                <a href="#haiphong" target="_blank">
                                    <img src="/img/haiphong.jpg" alt="Rounded Image" class="img-rounded img-responsive">
                                    <h5>Hải Phòng</h5>
                                </a>
                            </div>
                            <div class="col-md-4 one-location">
                                <a href="#danang" target="_blank">
                                    <img src="/img/danang.png" alt="Rounded Image" class="img-rounded img-responsive">
                                    <h5>Đà Nẵng</h5>
                                </a>
                            </div>
                        </div>
                        <!-- loai hinh duoc ua thich -->
                        <div class="row">
                            <div class="col-md-8 col-md-offset-2">
                                <h2>Popular trip's plan</h2>
                            </div>
                        </div>
                        <div class="row example-pages">
                            <div class="col-md-4 one-location">
                                <a href="#hanoi" target="_blank">
                                    <img src="/img/hoguom.jpg" alt="Rounded Image" class="img-rounded img-responsive">
                                    <h5>Hà Nội</h5>
                                </a>
                            </div>
                            <div class="col-md-4 one-location">
                                <a href="#haiphong" target="_blank">
                                    <img src="/img/haiphong.jpg" alt="Rounded Image" class="img-rounded img-responsive">
                                    <h5>Hải Phòng</h5>
                                </a>
                            </div>
                            <div class="col-md-4 one-location">
                                <a href="#danang" target="_blank">
                                    <img src="/img/danang.png" alt="Rounded Image" class="img-rounded img-responsive">
                                    <h5>Đà Nẵng</h5>
                                </a>
                            </div>
                        </div>
                    </div>
                </div>



                <div class="section landing-section">
                    <div class="container">
                        <div class="row">
                            <div class="col-md-8 col-md-offset-2">
                                <h2 class="text-center">Registry now?</h2>
                                <form class="contact-form" action="/register" method="POST">
                                    <div class="row">
                                        <div class="col-md-12">
                                            <label>Name</label>
                                            <input required class="form-control" name="name" placeholder="Name">
                                        </div>
                                        <div class="col-md-6">
                                            <label>Email</label>
                                            <input required type="email" name="username" class="form-control"
                                                placeholder="Email">
                                        </div>
                                        <div class="col-md-6">
                                            <label>Password</label>
                                            <input required type="password" class="form-control" placeholder="Password"
                                                name="password">
                                        </div>
                                        <div class="col-md-6">
                                            <label>Gender</label>
                                            <select class="form-control" name="gender">
                                                <option value="2" selected>Female</option>
                                                <option value="1">Male</option>
                                                <option value="0">Other</option>
                                            </select>
                                        </div>
                                        <div class="col-md-6">
                                            <label>Age</label>
                                            <input required type="number" name="age" class="form-control"
                                                placeholder="Enter your age">
                                        </div>
                                    </div>
                                    <div class="row">
                                        <div class="col-md-4 col-md-offset-4">
                                            <input type="submit" class="btn btn-danger btn-block btn-lg btn-fill" value="Registry"/>
                                        </div>
                                    </div>
                                </form>
                            </div>
                        </div>

                    </div>
                </div>
            </div>
        </div>
    </div>


    <footer class="footer-demo section-dark">
        <div class="container">
            <nav class="pull-left">
                <ul>

                    <li>
                        <a href="#">
                            Create in 30 minutes
                        </a>
                    </li>
                    <li>
                        <a href="#">
                            Blog
                        </a>
                    </li>
                    <li>
                        <a href="#">
                            Licenses
                        </a>
                    </li>
                </ul>
            </nav>
        </div>
    </footer>

</body>

