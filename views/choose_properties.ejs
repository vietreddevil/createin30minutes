<!doctype html>
<html lang="en">

<head>
    <meta charset="utf-8" />
    <link rel="icon" type="image/png" href="/paper_img/favicon.ico">
    <meta http-equiv="X-UA-Compatible" content="IE=edge,chrome=1" />

    <title>Create in 30 minutes</title>

    <meta content='width=device-width, initial-scale=1.0, maximum-scale=1.0, user-scalable=0' name='viewport' />
    <meta name="viewport" content="width=device-width" />

    <link href="/bootstrap3/css/bootstrap.css" rel="stylesheet" />
    <link href="/css/ct-paper.css" rel="stylesheet" />
    <link href="/css/demo.css" rel="stylesheet" />

    <!--     Fonts and icons     -->
    <link href="http://maxcdn.bootstrapcdn.com/font-awesome/4.2.0/css/font-awesome.min.css" rel="stylesheet">
    <link href='http://fonts.googleapis.com/css?family=Montserrat' rel='stylesheet' type='text/css'>
    <link href='http://fonts.googleapis.com/css?family=Open+Sans:400,300' rel='stylesheet' type='text/css'>
    <style>
        body {
            background-color: #d6d2cc;
            background-image: url('https://res.klook.com/image/upload/fl_lossy.progressive,q_85/c_fill,,w_1920,/v1556780466/banner/wg7sgxppdf51wfyprqtc.webp');
            background-size: cover;
        }

        .createplan {
            position: absolute;
            left: 47%;
            top: calc(100% - 120px);
            z-index: 4;
        }

        .checkbox {
            text-align: center
        }

        .checkbox img {
            width: 290px;
            height: 290px;
        }
    </style>
</head>

<body onresize="configview();">
    <nav class="navbar navbar-default" role="navigation-demo" id="demo-navbar">
        <div class="container">
            <!-- Brand and toggle get grouped for better mobile display -->
            <div class="navbar-header">
                <button type="button" class="navbar-toggle" data-toggle="collapse" data-target="#navigation-example-2">
                    <span class="sr-only">Toggle navigation</span>
                    <span class="icon-bar"></span>
                    <span class="icon-bar"></span>
                    <span class="icon-bar"></span>
                </button>
                <a class="navbar-brand" href="#">Create in 30 minutes</a>
            </div>

            <!-- Collect the nav links, forms, and other content for toggling -->
            <div class="collapse navbar-collapse" id="navigation-example-2">
                <ul class="nav navbar-nav navbar-right">
                        <li>
                                <a onclick="getLocation();" class="btn btn-simple">
                                    <i class="fa fa-map-marker"></i></a>
                            </li>
                    <li>
                        <a href="/profile" class="btn btn-simple">
                            <i class="fa fa-user"></i></a>
                    </li>
                    <li class="nav-item">
                        <a class="nav-link btn-rotate" href="/myplan" title="My plan">
                            <i class="fa fa-bar-chart" aria-hidden="true"></i>
                            <p>
                                <span class="d-lg-none d-md-block">Plan</span>
                            </p>
                        </a>
                    </li>
                </ul>
            </div><!-- /.navbar-collapse -->
        </div><!-- /.container-->
    </nav>

    <div id="container">
        <div id="carousel">
            <div id="carousel-example-generic" class="carousel slide" data-ride="carousel">
                <form action="/aftercreate" method="POST">
                    <div class="carousel slide" data-ride="carousel">

                        <!-- Indicators -->
                        <ol class="carousel-indicators">
                            <li data-target="#carousel-example-generic" data-slide-to="0" class="active"></li>
                            <li data-target="#carousel-example-generic" data-slide-to="1"></li>
                            <li data-target="#carousel-example-generic" data-slide-to="2"></li>
                        </ol>

                        <!-- Wrapper for slides -->
                        <div class="carousel-inner">
                            <div class="item active">
                                <div class="card row">
                                    <div class="col-sm-6">
                                        <label class="checkbox">
                                            <img class="col-sm-12" src="/img/kid.jpg"><br>
                                            Good for kids
                                            <input type="checkbox" name="travel_type" data-toggle="checkbox" id="prio1"
                                                value="Good for Kids">
                                            <i></i>
                                        </label>
                                    </div>
                                    <div class="col-sm-6">
                                        <label class="checkbox">
                                            <img class="col-sm-12" src="/img/money.jpg"><br>
                                            Budget-friendly
                                            <input type="checkbox" name="travel_type" data-toggle="checkbox" id="prio2"
                                                value="Budget-friendly">
                                            <i></i>
                                        </label>
                                    </div>
                                </div>
                            </div>
                            <div class="item">
                                <div class="card row">
                                    <div class="col-sm-6">
                                        <label class="checkbox">
                                            <img class="col-sm-12" src="/img/group.jpg"><br>
                                            Good for Big Groups
                                            <input type="checkbox" name="travel_type" data-toggle="checkbox" id="prio1"
                                                value="Good for Big Groups">
                                            <i></i>
                                        </label>
                                    </div>
                                    <div class="col-sm-6">
                                        <label class="checkbox">
                                            <img class="col-sm-12" src="/img/couple.jpg"><br>
                                            Good for Couples
                                            <input type="checkbox" name="travel_type" data-toggle="checkbox" id="prio2"
                                                value="Good for Couples">
                                            <i></i>
                                        </label>
                                    </div>
                                </div>
                            </div>
                            <div class="item">
                                <div class="card row">
                                    <div class="col-sm-6">
                                        <label class="checkbox">
                                            <img class="col-sm-12" src="/img/freeentry.jpg"><br>
                                            Free Entry
                                            <input type="checkbox" name="travel_type" data-toggle="checkbox" id="prio1"
                                                value="Free Entry">
                                            <i></i>
                                        </label>
                                    </div>
                                    <div class="col-sm-6">
                                        <label class="checkbox">
                                            <img class="col-sm-12" src="/img/AdrenalineSeekers.jpg"><br>
                                            Good for Adrenaline Seekers
                                            <input type="checkbox" name="travel_type" data-toggle="checkbox" id="prio2"
                                                value="Good for Adrenaline Seekers">
                                            <i></i>
                                        </label>
                                    </div>
                                </div>
                            </div>
                            <div class="item">
                                <div class="card row">
                                    <div class="col-sm-6">
                                        <label class="checkbox">
                                            <img class="col-sm-12" src="/img/RainyDay.jpg"><br>
                                            Good for a Rainy Day
                                            <input type="checkbox" name="travel_type" data-toggle="checkbox" id="prio1"
                                                value="Good for a Rainy Day">
                                            <i></i>
                                        </label>
                                    </div>
                                    <div class="col-sm-6">
                                        <label class="checkbox">
                                            <img class="col-sm-12" src="/img/Honeymoonspot.jpg"><br>
                                            Honeymoon spot
                                            <input type="checkbox" name="travel_type" data-toggle="checkbox" id="prio2"
                                                value="Honeymoon spot">
                                            <i></i>
                                        </label>
                                    </div>
                                </div>
                            </div>
                            <div class="item">
                                <div class="card row">
                                    <div class="col-sm-6">
                                        <label class="checkbox">
                                            <img class="col-sm-12" src="/img/Adventurous.jpg"><br>
                                            Adventurous
                                            <input type="checkbox" name="travel_type" data-toggle="checkbox" id="prio1"
                                                value="Adventurous">
                                            <i></i>
                                        </label>
                                    </div>
                                    <div class="col-sm-6">
                                        <label class="checkbox">
                                            <img class="col-sm-12" src="/img/HiddenGems.jpg"><br>
                                            Hidden Gems
                                            <input type="checkbox" name="travel_type" data-toggle="checkbox" id="prio2"
                                                value="Hidden Gems">
                                            <i></i>
                                        </label>
                                    </div>
                                </div>
                            </div>
                        </div>

                        <!-- Controls -->
                        <a class="left carousel-control" href="#carousel-example-generic" data-slide="prev">
                            <span class="fa fa-angle-left"></span>
                        </a>
                        <a class="right carousel-control" href="#carousel-example-generic" data-slide="next">
                            <span class="fa fa-angle-right"></span>
                        </a>
                    </div>
                    <input type="submit" class="btn btn-fill btn-warning createplan" value="Create">
                </form>
            </div>
        </div>
    </div>

</body>

<script src="/js/jquery-1.10.2.js" type="text/javascript"></script>
<script src="/js/jquery-ui-1.10.4.custom.min.js" type="text/javascript"></script>

